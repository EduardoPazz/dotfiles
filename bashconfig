set -o vi

unalias -a

alias graphs="startx"
alias ls='ls --color=auto'
alias ll='ls -oA'
alias apod='lynx https://apod.nasa.gov/apod/'
alias vi=vim
alias ?=duck
alias todos="grep --color -Ern 'TODO:|FIXME:'"
alias pwdc="keepassxc-cli clip -k $HOME/.passdb.key"

export PATH=\
$HOME/repos/gitlab.com/_joao.fukuda/dotfiles/scripts/:\
$HOME/.bin/:\
$PATH:

export CDPATH=\
./:\
$HOME/:\
$HOME/repos/gitlab.com/_joao.fukuda/:\
$HOME/repos/gitlab.com/joao.fukuda/:\
$HOME/repos/github.com/joaofukuda/:\
$HOME/Documents:

